{
	"$schema": "https://turborepo.com/schema.json",
	"ui": "tui",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"inputs": ["$TURBO_DEFAULT$", ".env*"],
			"outputs": [".next/**", "!.next/cache/**"]
		},
		"lint": {
			"dependsOn": ["^lint", "build"],
			"outputs": [".cache/.eslintcache"]
		},
		"check-types": {
			"dependsOn": ["^check-types"]
		},
		"dev": {
			"cache": false,
			"persistent": true
		}
	},
	"globalEnv": [
		"DATABASE_URL_NETWORK",
		"DATABASE_URL",
		"TWITCH_CLIENT_ID",
		"TWITCH_CLIENT_SECRET",
		"GOOGLE_CLIENT_ID",
		"GOOGLE_CLIENT_SECRET",
		"KICK_CLIENT_ID",
		"KICK_CLIENT_SECRET",
		"BETTER_AUTH_URL",
		"BETTER_AUTH_SECRET",
		"NEXT_PUBLIC_APP_URL",
		"CASHFREE_API_VERSION",
		"CASHFREE_ENVIRONMENT",
		"CASHFREE_CLIENT_ID",
		"CASHFREE_CLIENT_SECRET",
		"CASHFREE_WEBHOOK_SECRET",
		"NEXT_PUBLIC_KLIPY_WEB_API_KEY",
		"NEXT_PUBLIC_KLIPY_WEB_BASE_URL",
		"REDIS_URL"
	],
	"globalPassThroughEnv": [
		"NODE_ENV",
		"CI",
		"VERCEL",
		"VERCEL_ENV",
		"VERCEL_URL",
		"npm_lifecycle_event"
	]
}
